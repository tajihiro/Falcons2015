<!-- Game Navi -->
<%= render partial: 'application/games_navi', selected_game:@game, games_navi:@games_navi %>
<!--// Game Navi -->

<p></p>

<!-- Game Table-->
<h4>Game</h4>
<%=form_for @game do |f| %>
<div>
    <div class="row">
        <div class="col-md-12">
          <table class="table table-bordered">

            <thead>
            <tr>
              <th colspan="2">Date : <%= f.date_select :game_date, {:use_month_numbers => true} %></th>
              <th colspan="6">Time : <span><%= f.time_select :game_time, {:minute_step => 30, :time_separator => ""} %></span></th>
              <th colspan="4">Place : <%= f.text_field :game_place %></th>
            </tr>
            <tr>
              <th colspan="12">Div :
                <% @divisions.each do |division|%>
                    <%= division.division_en_name %>
                    <%= f.radio_button :division_id, division.id %> /
                <% end %>
              </th>
            </tr>
            <tr>
              <th>Goal</th>
              <th colspan="2">Team</th>
              <th>1</th>
              <th>2</th>
              <th>OT</th>
              <th>Total(P)</th>
              <th>SOG</th>
              <th>1</th>
              <th>2</th>
              <th>OT</th>
              <th>Total(S)</th>
            </tr>
            </thead>
            <tbody>
            <tr>
              <th>Visitor</th>
              <!--Away Team-->
              <td colspan="2">
                <%= f.collection_select(:away_team_id, @season_teams, :team_id, :team_name) %>
              </td>
              <!--1-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--2-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--OT-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--Total(P)-->
              <td>
                <%= text_field_tag :away_team_score, @game.away_team_score, {size:2, maxlength:3, style:'text-align:right;'} %>
              </td>
              <!--SOG-->
              <td>-</td>
              <!--1-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--2-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--OT-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--Total(S)-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
            </tr>
            <tr>
              <th>Home</th>
              <!--Home Team-->
              <td colspan="2">
                <%= f.collection_select(:home_team_id, @season_teams, :team_id, :team_name) %>
              </td>
              <!--1-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--2-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--OT-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--Total(P)-->
              <td>
                <%= text_field_tag :away_team_score, @game.home_team_score, {size:2, maxlength:3, style:'text-align:right;'} %>
              </td>
              <!--SOG-->
              <td>-</td>
              <!--1-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--2-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--OT-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
              <!--Total-->
              <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
            </tr>
            </tbody>
          </table>
        </div>
    </div>
</div>
<% end %>
<!--// Game Table-->


<!-- Roster Table-->
<h4>Roster</h4>
<div>
    <div class="row">
      <div class="col-md-12">
        <table class="table table-bordered table-striped">
          <thead>
          <tr>
            <th>#</th>
            <th>Pos.</th>
            <th>Name</th>
            <th>前半</th>
            <th>後半</th>
            <th>OT</th>
            <th>MVP</th>
          </tr>
          </thead>
          <!--Goalee-->
          <tbody>
          <% @game_goalies.each do |goalie| %>
              <tr>
                <!-- # -->
                <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
                <!-- G -->
                <td><%= goalie.position_name %></td>
                <!-- Name -->
                <td><span><%= goalie.goalie_name %></span></td>
                <!-- 前半 -->
                <td><input type="checkbox" checked></td>
                <!-- 後半 -->
                <td><input type="checkbox" checked></td>
                <!-- OT -->
                <td><input type="checkbox" ></td>
                <!-- MVP -->
                <td><input type="checkbox" ></td>
              </tr>
          <% end %>
          <tr>
            <!-- # -->
            <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
            <!-- G -->
            <td>G</td>
            <!-- Name -->
            <td><span></span></td>
            <!-- 前半 -->
            <td><input type="checkbox"></td>
            <!-- 後半 -->
            <td><input type="checkbox"></td>
            <!-- OT -->
            <td><input type="checkbox" ></td>
            <!-- MVP -->
            <td><input type="checkbox" ></td>
          </tr>
          </tbody>
          <!--//Goalee-->

          <!--Player-->
          <thead>
          <tr>
            <th>#</th>
            <th>Pos.</th>
            <th>Name</th>
            <th>G</th>
            <th>A</th>
            <th>P</th>
            <th>MVP</th>
          </tr>
          </thead>
          <tbody>
          <% @game_players.each do |player| %>
          <tr>
            <!-- # -->
            <td><input type="text" size="2" maxlength="3" style="text-align:right;" value="<%= player.member_id %>"></td>
            <!-- Pos. -->
            <td><span><%= player.position_name %></span></td>
            <!-- Name -->
            <td><span><%= player.player_name %></span></td>
            <!-- G -->
            <td><input type="text" size="2" maxlength="3" style="text-align:right;" value="<%= player.goal %>"></td>
            <!-- A -->
            <td><input type="text" size="2" maxlength="3" style="text-align:right;" value="<%= player.assist %>"></td>
            <!-- P -->
            <td><input type="text" size="2" maxlength="3" style="text-align:right;" value="<%= player.point %>"></td>
            <!-- MVP -->
            <td><input type="text" size="2" maxlength="3" style="text-align:right;" value="<%= player.mvp_flg %>"></td>
          </tr>
          <% end %>
          <tr>
            <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
            <td><span></span></td>
            <td><span></span></td>
            <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
            <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
            <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
            <td><input type="text" size="2" maxlength="3" style="text-align:right;"></td>
          </tr>
          </tbody>
          <!--//Player-->
        </table>
      </div>
    </div>
</div>
<!--// Roster Table-->

